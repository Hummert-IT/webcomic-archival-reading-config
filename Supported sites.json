{
	"qwantz.com": {
		"nav": {
			"prev":  "a[rel=prev]",
			"next":  "a[rel=next]"
		},
		"alt": {
			"comic": "img.comic",
			"style": "background:rgba(255,255,255,.8);padding:7px;margin:7px 0;"
		}
	},
	"daisyowl.com": {
		"nav": {
			"prev":  ".nav > a:first-of-type",
			"next":  ".nav > a:last-of-type"
		},
		"alt": {
			"comic": ".main > div:nth-of-type(2) img",
			"style": "padding:7px;margin: 0 0 20px;"
		}
	},
	"xkcd.com": {
		"nav": {
			"prev":  ".comicNav a[rel=prev]",
			"next":  ".comicNav a[rel=next]"
		},
		"alt": {
			"comic": "#comic > img",
			"style": "margin:7px auto 30px;text-decoration: none !important;"
		}
	},
	"what-if.xkcd.com": {
		"nav": {
			"prev": ".nav-prev > a",
			"next": ".nav-next > a"
		},
		"alt": {
			"comic": "img.illustration",
			"style": "margin:0 auto;text-decoration: none !important;font-style: oblique;"
		},
		"exp": {
			"source": ".refbody",
			"destin": ".refnum",
			"prefix": " [",
			"suffix": "] ",
			"style": "bottom:0;text-decoration: none;cursor: default;font-size: .75em;"
		}
	},
	"drmcninja.com": {
		"nav": {
			"prev":  ".prepostnav > a.prev",
			"next":  ".prepostnav > a.next"
		},
		"alt": {
			"comic": "#comic > img",
			"ignore": ["[0-9]+?p[0-9]+"],
			"style": "margin:0 0 30px;text-decoration: none !important;"
		}
	},
	"smbc-comics.com": {
		"nav": {
			"prev":  ".nav > a[rel=prev]",
			"next":  ".nav > a[rel=next]"
		},
		"alt": {
			"comic": "#cc-comic",
			"after": "#aftercomic img",
			"ignore": ["[0-9][0-9][0-9][0-9]-[0-1][0-9]-[0-3][0-9]"],
			"style": "margin:0 0 16px;text-decoration: none !important;"
		}
	},
	"mspaintadventures.com": {

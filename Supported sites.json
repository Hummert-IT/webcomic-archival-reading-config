		},
		"exp": {
			"source": ".spoiler > table",
			"destin": "html > body > center > table > tbody > tr > td > table > tbody > tr > td > table > tbody > tr > td > center > table > tbody > tr > td > table > tbody > tr > td > center > div",
			"style": "text-align: left;"
		}
